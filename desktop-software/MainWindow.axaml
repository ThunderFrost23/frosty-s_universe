		  RowDefinitions="20, Auto, 40">

		<!-- HEADER -->		
		<Border Grid.Row="0"
				Background="#292f36"
				CornerRadius="25"
				Height="100">
			<TextBlock Foreground="#508991"
					   FontSize="25">
				ARTISTS
			</TextBlock>
		</Border>
	</Grid>
</Window>
